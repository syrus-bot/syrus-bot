{
	"name": "syrus",
	"version": "1.0.0",
	"description": "Syrus - a multipurpose Discord bot, designed to be the best so you won't need the rest.",
	"main": "src/main.js",
	"scripts": {
		"start": "npm run lint && cd src && node main",
		"test": "npx nyc --reporter=lcov mocha --recursive test/**/*.js",
		"lint": "npx eslint src --ext .json,.js --fix",
		"gpr": "git pull && npm start",
		"commit": "git add . && npx git cz"
	},
	"author": {
		"name": "Nytelife26 (Tyler J. Russell)",
		"email": "nytelife@protonmail.com",
		"url": "https://github.com/Nytelife26"
	},
	"contributors": [
		{
			"name": "ColeCCI",
			"email": "hi@colem.dev",
			"url": "https://colem.dev"
		},
		{
			"name": "mynameismax",
			"email": "max@maxx.lol",
			"url": "https://mrti.me"
		}
	],
	"license": "MIT",
	"dependencies": {
		"@sapphire/discord-utilities": "^1.0.3",
		"@sapphire/framework": "^1.0.0-alpha.5",
		"@sapphire/pieces": "^1.0.1",
		"@sapphire/plugin-i18next": "^1.2.1",
		"@sapphire/plugin-logger": "^1.0.6",
		"@sapphire/time-utilities": "^1.2.0",
		"@types/ws": "^7.4.0",
		"bufferutil": "^4.0.2",
		"discord.js": "^12.5.1",
		"globby": "^11.0.1",
		"got": "^11.8.1",
		"lavaqueue": "^3.1.8",
		"module-alias": "^2.2.2",
		"mongoose": "^5.11.8"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/git": "^9.0.0",
		"all-contributors-cli": "^6.19.0",
		"commitizen": "^4.2.2",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.14.0",
		"eslint-plugin-json": "^2.1.2",
		"husky": "^4.3.5",
		"mocha": "^8.2.1",
		"mongodb-memory-server": "^6.9.2",
		"nyc": "^15.1.0",
		"semantic-release": "^17.3.1",
		"sinon": "^9.2.2"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint",
			"pre-push": "npm run lint && npm test",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"_moduleAliases": {
		"@sky": "./src",
		"@flares": "./src/flares",
		"@struct": "./src/lib/structures",
		"@exts": "./src/lib/extensions",
		"@data": "./src/data"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/syrus-bot/syrus-bot.git"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"bugs": {
		"url": "https://github.com/syrus-bot/syrus-bot/issues"
	},
	"homepage": "https://github.com/syrus-bot/syrus-bot#readme"
}
